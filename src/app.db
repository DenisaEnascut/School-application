-- Crearea tabelului "Clase"
CREATE TABLE Clase (
  id_clasa INTEGER PRIMARY KEY,
  nume_clasa VARCHAR(50),
  nume_profesor VARCHAR(50)
);

-- Crearea tabelului "Studenti"
CREATE TABLE Studenti (
  id_student INTEGER PRIMARY KEY,
  nume_student VARCHAR(50),
  id_clasa INTEGER,
  FOREIGN KEY (id_clasa) REFERENCES Clase(id_clasa)
);

-- Crearea tabelului "Profesori"
CREATE TABLE Profesori (
  id_profesor INTEGER PRIMARY KEY,
  nume_profesor VARCHAR(50)
);

-- Crearea tabelului "Note"
CREATE TABLE Note (
  id_nota INTEGER PRIMARY KEY,
  id_student INTEGER,
  id_clasa INTEGER,
  valoare_nota INTEGER,
  FOREIGN KEY (id_student) REFERENCES Studenti(id_student)
);

